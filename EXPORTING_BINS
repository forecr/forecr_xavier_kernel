	cp $HOME/kernel_out_milboard_xv/arch/arm64/boot/dts/nvidia/tegra234-p3701-0000-p3737-0000-milboard-xv.dtb $HOME/kernel_out_milboard_xv_agx_orin/tegra234-p3701-0000-p3737-0000.dtb
	cp $HOME/kernel_out_milboard_xv/arch/arm64/boot/dts/nvidia/tegra234-p3701-0004-p3737-0000-milboard-xv.dtb $HOME/kernel_out_milboard_xv_agx_orin/tegra234-p3701-0004-p3737-0000.dtb
	cp $HOME/kernel_out_milboard_xv/arch/arm64/boot/dts/nvidia/tegra234-p3701-0005-p3737-0000-milboard-xv.dtb $HOME/kernel_out_milboard_xv_agx_orin/tegra234-p3701-0005-p3737-0000.dtb
	make ARCH=arm64 O=$HOME/kernel_out_milboard_xv modules_install INSTALL_MOD_PATH=$INSTALL_MOD_PATH INSTALL_MOD_STRIP=1
	cp $INSTALL_MOD_PATH/lib/modules/5.10.192-tegra/kernel/drivers/gpu/nvgpu/nvgpu.ko $HOME/kernel_out_milboard_xv_agx_orin/
	rm -rf $INSTALL_MOD_PATH



