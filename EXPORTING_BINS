# For DSBOARD-AGX with AGX Orin SoM, type these commands below:
	mkdir $HOME/kernel_out_dsboard_agx_orin
	cp $KERNEL_OUTPUT/arch/arm64/boot/Image $HOME/kernel_out_dsboard_agx_orin/Image
	cp kernel-devicetree/generic-dts/dtbs/tegra234-p3737-0000+p3701-0000-nv-dsboard-agx-base.dtb $HOME/kernel_out_dsboard_agx_orin/tegra234-p3737-0000+p3701-0000-nv.dtb
	cp kernel-devicetree/generic-dts/dtbs/tegra234-p3737-0000+p3701-0004-nv-dsboard-agx-base.dtb $HOME/kernel_out_dsboard_agx_orin/tegra234-p3737-0000+p3701-0004-nv.dtb
	cp kernel-devicetree/generic-dts/dtbs/tegra234-p3737-0000+p3701-0005-nv-dsboard-agx-base.dtb $HOME/kernel_out_dsboard_agx_orin/tegra234-p3737-0000+p3701-0005-nv.dtb
	cp kernel-devicetree/generic-dts/dtbs/tegra234-p3737-0000+p3701-0000-nv-dsboard-agx.dtb $HOME/kernel_out_dsboard_agx_orin/0_tegra234-p3737-0000+p3701-0000-nv.dtb
	cp kernel-devicetree/generic-dts/dtbs/tegra234-p3737-0000+p3701-0004-nv-dsboard-agx.dtb $HOME/kernel_out_dsboard_agx_orin/0_tegra234-p3737-0000+p3701-0004-nv.dtb
	cp kernel-devicetree/generic-dts/dtbs/tegra234-p3737-0000+p3701-0005-nv-dsboard-agx.dtb $HOME/kernel_out_dsboard_agx_orin/0_tegra234-p3737-0000+p3701-0005-nv.dtb
	cp kernel-devicetree/generic-dts/dtbs/tegra234-p3737-0000+p3701-0000-dynamic.dtbo $HOME/kernel_out_dsboard_agx_orin/
	export INSTALL_MOD_PATH=$HOME/nvidia/nvidia_sdk/JetPack_6.1_Linux_JETSON_AGX_ORIN_TARGETS/Linux_for_Tegra/rootfs/
	cd $INSTALL_MOD_PATH/..
	sudo ./apply_binaries.sh
	cd $OLDPWD
	sudo -E make install -C kernel
	cd $INSTALL_MOD_PATH
	sudo rm -rf ./lib/modules/5.15.148-tegra/build
	sudo rm -rf ./lib/modules/5.15.148-tegra/source
	sudo tar --owner root --group root -cjf kernel_supplements.tbz2 lib/modules
	sync
	cd $OLDPWD
	cp $INSTALL_MOD_PATH/kernel_supplements.tbz2 $HOME/kernel_out_dsboard_agx_orin/


