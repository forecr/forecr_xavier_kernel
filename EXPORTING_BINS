# For DSBOARD_XV2 with AGX Xavier SoM, type these commands below:
	mkdir $HOME/kernel_out_dsboard_xv2_agx
	cp $HOME/kernel_out_dsboard_xv2/arch/arm64/boot/Image $HOME/kernel_out_dsboard_xv2_agx/Image
	cp $HOME/kernel_out_dsboard_xv2/arch/arm64/boot/dts/nvidia/tegra194-p2888-0001-dsboard-xv2-0000.dtb $HOME/kernel_out_dsboard_xv2_agx/tegra194-p2888-0001-p2822-0000.dtb
	mkdir $HOME/kernel_out_dsboard_xv2_agx/imx477
	cp $HOME/kernel_out_dsboard_xv2/arch/arm64/boot/dts/nvidia/tegra194-p2888-0001-dsboard-xv2-0000-imx477.dtb $HOME/kernel_out_dsboard_xv2_agx/imx477/tegra194-p2888-0001-p2822-0000.dtb
	mkdir $HOME/kernel_out_dsboard_xv2_agx/xv2-expansionboard
	cp $HOME/kernel_out_dsboard_xv2/arch/arm64/boot/dts/nvidia/tegra194-p2888-0001-dsboard-xv2-0000-expansion.dtb $HOME/kernel_out_dsboard_xv2_agx/xv2-expansionboard/tegra194-p2888-0001-p2822-0000.dtb
	make ARCH=arm64 O=$HOME/kernel_out_dsboard_xv2 modules_install INSTALL_MOD_PATH=$INSTALL_MOD_PATH INSTALL_MOD_STRIP=1
	cp $INSTALL_MOD_PATH/lib/modules/5.10.192-tegra/kernel/drivers/gpu/nvgpu/nvgpu.ko $HOME/kernel_out_dsboard_xv2_agx/
	rm -rf $INSTALL_MOD_PATH


