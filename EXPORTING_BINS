	cp $HOME/kernel_out_dsboard_nx2/arch/arm64/boot/dts/nvidia/tegra194-p3668-dsboard-nx2-0000.dtb $HOME/kernel_out_dsboard_nx2_xavier/tegra194-p3668-0001-p3509-0000.dtb
	mkdir $HOME/kernel_out_dsboard_nx2_xavier/extra_dtbs
	mkdir $HOME/kernel_out_dsboard_nx2_xavier/extra_dtbs/imx477
	cp $HOME/kernel_out_dsboard_nx2/arch/arm64/boot/dts/nvidia/tegra194-p3668-dsboard-nx2-0000-imx477.dtb $HOME/kernel_out_dsboard_nx2_xavier/extra_dtbs/imx477/tegra194-p3668-0001-p3509-0000.dtb
	mkdir $HOME/kernel_out_dsboard_nx2_xavier/without_fan
	cp $HOME/kernel_out_dsboard_nx2/arch/arm64/boot/dts/nvidia/tegra194-p3668-dsboard-nx2-0000-rev121.dtb $HOME/kernel_out_dsboard_nx2_xavier/without_fan/tegra194-p3668-0001-p3509-0000.dtb
