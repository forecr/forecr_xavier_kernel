# For DSBOARD_XV2 with AGX Xavier SoM, type these commands below:
	mkdir $HOME/kernel_out_dsboard_xv2_agx
	cp $HOME/kernel_out_dsboard_xv2/drivers/gpu/nvgpu/nvgpu.ko $HOME/kernel_out_dsboard_xv2_agx/
	echo "Linux_for_Tegra/rootfs/usr/lib/modules/\$(uname -r)/kernel/drivers/gpu/nvgpu/nvgpu.ko" > $HOME/kernel_out_dsboard_xv2_agx/mv_nvgpu_ko.txt
	cp $HOME/kernel_out_dsboard_xv2/arch/arm64/boot/Image $HOME/kernel_out_dsboard_xv2_agx/Image 
	cp $HOME/kernel_out_dsboard_xv2/arch/arm64/boot/dts/nvidia/tegra194-p2888-0001-dsboard-xv2-0000.dtb $HOME/kernel_out_dsboard_xv2_agx/tegra194-p2888-0001-p2822-0000.dtb


# For DSBOARD_XV2 with AGX Xavier Industrial SoM, type these commands below:
	mkdir $HOME/kernel_out_dsboard_xv2_agxi
	cp $HOME/kernel_out_dsboard_xv2/drivers/gpu/nvgpu/nvgpu.ko $HOME/kernel_out_dsboard_xv2_agxi/
	echo "Linux_for_Tegra/rootfs/usr/lib/modules/\$(uname -r)/kernel/drivers/gpu/nvgpu/nvgpu.ko" > $HOME/kernel_out_dsboard_xv2_agxi/mv_nvgpu_ko.txt
	cp $HOME/kernel_out_dsboard_xv2/arch/arm64/boot/Image $HOME/kernel_out_dsboard_xv2_agxi/Image 
	cp $HOME/kernel_out_dsboard_xv2/arch/arm64/boot/dts/nvidia/tegra194-p2888-0008-dsboard-xv2-0000.dtb $HOME/kernel_out_dsboard_xv2_agxi/tegra194-p2888-0008-p2822-0000.dtb


# For DSBOARD_AGX with AGX Orin SoM, type these commands below:
	mkdir $HOME/kernel_out_dsboard_agx_orin
	cp $HOME/kernel_out_dsboard_agx/drivers/gpu/nvgpu/nvgpu.ko $HOME/kernel_out_dsboard_agx_orin/
	echo "Linux_for_Tegra/rootfs/usr/lib/modules/\$(uname -r)/kernel/drivers/gpu/nvgpu/nvgpu.ko" > $HOME/kernel_out_dsboard_agx_orin/mv_nvgpu_ko.txt
	cp $HOME/kernel_out_dsboard_agx/arch/arm64/boot/Image $HOME/kernel_out_dsboard_agx_orin/Image 
	cp $HOME/kernel_out_dsboard_agx/arch/arm64/boot/dts/nvidia/tegra234-p3701-0000-p3737-0000-dsboard-agx.dtb $HOME/kernel_out_dsboard_agx_orin/tegra234-p3701-0000-p3737-0000.dtb
	cp $HOME/kernel_out_dsboard_agx/arch/arm64/boot/dts/nvidia/tegra234-p3701-0004-p3737-0000-dsboard-agx.dtb $HOME/kernel_out_dsboard_agx_orin/tegra234-p3701-0004-p3737-0000.dtb


# For DSBOARD_ORNX with Orin NX SoM, type these commands below:
	mkdir $HOME/kernel_out_dsboard_ornx_orin_nx
	cp $HOME/kernel_out_dsboard_ornx/drivers/gpu/nvgpu/nvgpu.ko $HOME/kernel_out_dsboard_ornx_orin_nx/
	echo "Linux_for_Tegra/rootfs/usr/lib/modules/\$(uname -r)/kernel/drivers/gpu/nvgpu/nvgpu.ko" > $HOME/kernel_out_dsboard_ornx_orin_nx/mv_nvgpu_ko.txt
	cp $HOME/kernel_out_dsboard_ornx/arch/arm64/boot/Image $HOME/kernel_out_dsboard_ornx_orin_nx/Image
	cp $HOME/kernel_out_dsboard_ornx/arch/arm64/boot/dts/nvidia/tegra234-p3767-0000-p3768-0000-a0-dsboard-ornx.dtb $HOME/kernel_out_dsboard_ornx_orin_nx/tegra234-p3767-0000-p3768-0000-a0.dtb

# For DSBOARD_NX2 (rev-1.22) with Xavier NX SoM, type these commands below:
	mkdir $HOME/kernel_out_dsboard_nx2_xavier
	cp $HOME/kernel_out_dsboard_nx2/drivers/gpu/nvgpu/nvgpu.ko $HOME/kernel_out_dsboard_nx2_xavier/
	echo "Linux_for_Tegra/rootfs/usr/lib/modules/\$(uname -r)/kernel/drivers/gpu/nvgpu/nvgpu.ko" > $HOME/kernel_out_dsboard_nx2_xavier/mv_nvgpu_ko.txt
	cp $HOME/kernel_out_dsboard_nx2/arch/arm64/boot/Image $HOME/kernel_out_dsboard_nx2_xavier/Image
	cp $HOME/kernel_out_dsboard_nx2/arch/arm64/boot/dts/nvidia/tegra194-p3668-dsboard-nx2-0000.dtb $HOME/kernel_out_dsboard_nx2_xavier/tegra194-p3668-0001-p3509-0000.dtb
	mkdir $HOME/kernel_out_dsboard_nx2_xavier/imx477
	cp $HOME/kernel_out_dsboard_nx2/arch/arm64/boot/dts/nvidia/tegra194-p3668-dsboard-nx2-0000-imx477.dtb $HOME/kernel_out_dsboard_nx2_xavier/imx477/tegra194-p3668-0001-p3509-0000.dtb


