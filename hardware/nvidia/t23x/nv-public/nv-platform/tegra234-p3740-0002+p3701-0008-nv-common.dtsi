/ {
	aliases {
		serial2 = "/bus@0/serial@3110000";
	};

	chosen {
		bootargs = "console=ttyTCU0,115200n8";
	};

	cpus {
		idle-states {
			c7 {
				status = "okay";
			};
		};
	};

	nvpmodel {
		status = "okay";
	};

	soctherm-oc-event {
		status = "okay";
	};

	thermal-zones {
		cpu-thermal {
			cooling-maps {
				map-hot-surface-alert {
					cooling-device = <&hot_surface_alert 0 0>;
				};
			};
		};

		cv0-thermal {
			cooling-maps {
				map-hot-surface-alert {
