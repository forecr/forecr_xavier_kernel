/dts-v1/;

#include "tegra234-p3768-0000+p3767-0000.dts"
#include "tegra234-p3768-0000+p3767-xxxx-nv-common.dtsi"
#include "tegra234-p3768-0000-dsboard-ornxs.dtsi"

/ {
	nvidia,dtsfilename = __FILE__;
	nvidia,dtbbuildtime = __DATE__, __TIME__;
	model = "Orin NX 16GB for DSBOARD-ORNXS";

	bus@0 {
		padctl@3520000 {
			ports {
				usb2-0 {/* Goes to recovery port */
					mode = "host";
					/delete-property/ usb-role-switch;
					/delete-node/ connector;
				};
			};
		};
		usb@3550000 {
			status = "disabled";
		};
	};
};
