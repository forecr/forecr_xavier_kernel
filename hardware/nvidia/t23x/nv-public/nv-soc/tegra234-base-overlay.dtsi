				i2s@2901200 {
					#sound-dai-cells = <1>;
					nvidia,ahub-i2s-id = <2>;
				};

				i2s@2901300 {
					#sound-dai-cells = <1>;
					nvidia,ahub-i2s-id = <3>;
				};

				i2s@2901400 {
