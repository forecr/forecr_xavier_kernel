				};

				dspk@2905000 {
					#sound-dai-cells = <1>;
				};

				dspk@2905100 {
					#sound-dai-cells = <1>;
				};

				admaif@290f000 {
					#sound-dai-cells = <1>;
				};

				/*
				 * Below modules are upstreamed. DT device nodes
				 * are backported. But drivers are not in v5.15.
				 * Thus use existing downstream drivers and add
				 * '#sound-dai-cells' property needed for downstream
				 * machine driver.
