					port@6 {
						status = "disabled";
					};
					port@7 {
						status = "disabled";
					};
					port@8 {
						status = "disabled";
					};
					port@9 {
						status = "disabled";
					};
					port@10 {
						status = "disabled";
					};
					port@11 {
						status = "disabled";
					};
				};
			};
			bus@0 {
				host1x@13e00000 {
					nvcsi@15a00000 {
						num-channels = <2>;
						#address-cells = <1>;
						#size-cells = <0>;
						channel@0 {
							status = "okay";
						};
						channel@1 {
							status = "okay";
						};
						channel@2 {
							status = "disabled";
						};
						channel@3 {
							status = "disabled";
						};
						channel@4 {
							status = "disabled";
