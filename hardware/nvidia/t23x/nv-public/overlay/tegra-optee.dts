// Jetson Device-tree overlay for OP-TEE.

/dts-v1/;
/plugin/;

/ {
	overlay-name = "OP-TEE overlay";
	compatible = "nvidia,tegra234";

	fragment@0 {
		target-path = "/";
		board_config {
			sw-modules = "kernel";
		};
		__overlay__ {
			firmware {
				optee {
					compatible = "linaro,optee-tz";
					method = "smc";
					status = "disabled";
				};
			};
		};
	};
};
