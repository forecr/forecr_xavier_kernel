					step_gain_val = "1";
					default_gain = "16";
					
					min_hdr_ratio = "1";
					max_hdr_ratio = "1";
					min_framerate = "2000000"; /* 2.0 fps */
					max_framerate = "120000000"; /* 28.0 fps */
					step_framerate = "1";
					default_framerate = "7000000";
					
					min_exp_time = "13";
					max_exp_time = "683709";
					step_exp_time = "1";
					default_exp_time = "2495";

					embedded_metadata_height = "0";
				};
				mode2 { /* AR0234_MODE_1920x1200_60FPS */
